{
  "name": "Blackout",
  "version": "1.0.0",
  "description": "Take some time for mindfullness",
  "manifest_version": 3,
  "author": "Bryan Choo",
  "action": {
    "default_popup": "index.html",
    "default_title": "Blackout"
  },

  "background": {
    "service_worker": "background.js",
    "scripts": ["background.js", "main.js"]
  },
  "permissions": ["fullscreen","tabs"]
}
